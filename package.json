{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "Deploy a Gatsby Project to an AWS S3 bucket (optional Cloudfront support)",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonelantha/gatsby-s3-action.git"
  },
  "keywords": [
    "deploy",
    "gatsby",
    "aws",
    "s3",
    "cloudfront",
    "action"
  ],
  "author": "jonelantha",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/exec": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@typescript-eslint/parser": "^3.7.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.5.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.18.2",
    "jest": "^26.0.0",
    "jest-circus": "^26.0.0",
    "js-yaml": "^3.14.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  }
}
